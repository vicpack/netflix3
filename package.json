{
  "name": "clone_netflix",
  "version": "0.0.1",
  "description": "Este es un clone de netflix sin fines de lucro\"",
  "main": "server.js",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "babel -d ./build . --ignore node_modules --copy-files",
    "start": "node ./build/server.js",
    "server": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "test": "jest --forceExit"
  },
  "keywords": [
    "netflix"
  ],
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules"
    ],
    "coverageReporters": [
      "lcov",
      "html",
      "json"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    }
  },
  "author": "victor morales",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.1",
    "node-modules": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.17.5"
  }
}
